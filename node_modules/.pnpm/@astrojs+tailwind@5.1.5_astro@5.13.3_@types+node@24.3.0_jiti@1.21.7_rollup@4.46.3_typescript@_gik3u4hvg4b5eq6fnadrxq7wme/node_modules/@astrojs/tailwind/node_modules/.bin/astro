#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/apgautham/Research/08_GIT/ap-gautham.github.io/node_modules/.pnpm/astro@5.13.3_@types+node@24.3.0_jiti@1.21.7_rollup@4.46.3_typescript@5.7.3_yaml@2.8.1/node_modules/astro/node_modules:/Users/apgautham/Research/08_GIT/ap-gautham.github.io/node_modules/.pnpm/astro@5.13.3_@types+node@24.3.0_jiti@1.21.7_rollup@4.46.3_typescript@5.7.3_yaml@2.8.1/node_modules:/Users/apgautham/Research/08_GIT/ap-gautham.github.io/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/apgautham/Research/08_GIT/ap-gautham.github.io/node_modules/.pnpm/astro@5.13.3_@types+node@24.3.0_jiti@1.21.7_rollup@4.46.3_typescript@5.7.3_yaml@2.8.1/node_modules/astro/node_modules:/Users/apgautham/Research/08_GIT/ap-gautham.github.io/node_modules/.pnpm/astro@5.13.3_@types+node@24.3.0_jiti@1.21.7_rollup@4.46.3_typescript@5.7.3_yaml@2.8.1/node_modules:/Users/apgautham/Research/08_GIT/ap-gautham.github.io/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../astro@5.13.3_@types+node@24.3.0_jiti@1.21.7_rollup@4.46.3_typescript@5.7.3_yaml@2.8.1/node_modules/astro/astro.js" "$@"
else
  exec node  "$basedir/../../../../../../astro@5.13.3_@types+node@24.3.0_jiti@1.21.7_rollup@4.46.3_typescript@5.7.3_yaml@2.8.1/node_modules/astro/astro.js" "$@"
fi
